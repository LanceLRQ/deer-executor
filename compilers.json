{
    "gcc": "/usr/bin/gcc %s -o %s -ansi -fno-asm -Wall -std=c11 -lm",
    "gcc_cpp": "/usr/bin/g++ %s -o %s -ansi -fno-asm -Wall -lm -std=c++11",
    "java": "/usr/bin/javac -encoding utf-8 %s -d %s",
    "go": "/usr/local/go/bin/go build -o %s %s",
    "node": "/usr/bin/node -c %s",
    "php": "/usr/bin/php -l -f %s",
    "ruby": "/usr/bin/ruby -c %s"
}